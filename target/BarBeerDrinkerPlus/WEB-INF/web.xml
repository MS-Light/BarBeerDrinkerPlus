 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SearchBeer</servlet-name>
  	<display-name>SearchBeer</display-name>
  	<description></description>
  	<servlet-class>rpc.SearchItem</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Login</servlet-name>
  	<display-name>Login</display-name>
  	<description></description>
  	<servlet-class>rpc.Login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Logout</servlet-name>
  	<display-name>Logout</display-name>
  	<description></description>
  	<servlet-class>rpc.Logout</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Register</servlet-name>
  	<display-name>Register</display-name>
  	<description></description>
  	<servlet-class>rpc.Register</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetBeer</servlet-name>
  	<display-name>GetBeer</display-name>
  	<description></description>
  	<servlet-class>rpc.GetBeer</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetDrinker</servlet-name>
  	<display-name>GetDrinker</display-name>
  	<description></description>
  	<servlet-class>rpc.GetDrinker</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetBeer</servlet-name>
  	<url-pattern>/getbeer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Logout</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Register</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SearchBeer</servlet-name>
  	<url-pattern>/searchbeer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetDrinker</servlet-name>
  	<url-pattern>/getdrinker</url-pattern>
  </servlet-mapping>
</web-app>
