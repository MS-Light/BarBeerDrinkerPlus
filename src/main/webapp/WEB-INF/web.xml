<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SearchBeer</servlet-name>
  	<display-name>SearchBeer</display-name>
  	<description></description>
  	<servlet-class>rpc.SearchBeer</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Calculate</servlet-name>
  	<display-name>Calculate</display-name>
  	<description></description>
  	<servlet-class>rpc.Calculate</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SearchBeer</servlet-name>
  	<url-pattern>/search</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Calculate</servlet-name>
  	<url-pattern>/calculate</url-pattern>
  </servlet-mapping>
</web-app>
